<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.timebook.mapper.HabitReportMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="top.timebook.model.HabitReport">
        <id column="id" property="id" />
        <result column="habitId" property="habitId" />
        <result column="habitResult" property="habitResult" />
        <result column="habitTime" property="habitTime" />
